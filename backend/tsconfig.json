{
    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./src",
        "baseUrl": ".",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "removeComments": true,
        "skipLibCheck": true,
        "paths": {
            "src/*": ["./src/*"],
            "routes/*": ["./src/routes/*"],
            "controllers/*": ["./src/controllers/*"],
            "utils/*": ["./src/utils/*"],
        }
    }
}